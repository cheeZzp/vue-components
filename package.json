    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^6.2.2",
    "mini-css-extract-plugin": "^0.4.1",
    "prettier": "^2.2.1",
    "style-loader": "^0.23.1",
    "url-loader": "^1.0.1",
    "vue-loader": "^15.7.0",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.46.0",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^3.3.10"
  }
